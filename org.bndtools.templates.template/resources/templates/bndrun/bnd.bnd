# Example Run Template

# Make sure to edit the name and category.
Provide-Capability: \
	org.bndtools.template;\
		org.bndtools.template=bndrun; \
		name=<projectName>; \
		category=Examples; \
		dir=template; \
		icon="${icondata}"

icondata: data:image/gif;base64,${base64;icon.gif}

-resourceonly
-includeresource: resources